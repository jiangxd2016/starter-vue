{
  "name": "pkg-name",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint --cache ./ --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "changelog": "tsx scripts/changelog.ts",
    "test:watch": "vitest --watch",
    "sizecheck": "npx vite-bundle-visualizer",
    "prerelease": "pnpm run lint && pnpm run build && pnpm run typecheck && pnpm run test",
    "release": "pnpm run changelog && pnpm run prerelease && bumpp package.json  --no-git-checks --commit --push --tag"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@estjs/eslint-config": "^2.1.5",
    "@estjs/tools": "^0.2.0",
    "@iconify-json/carbon": "^1.2.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.1",
    "@unocss/reset": "^66.5.9",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "picocolors": "^1.1.1",
    "pnpm": "^10.23.0",
    "sass": "1.91.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "unocss": "^66.5.9",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-pages": "^0.33.1",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec tsx ./scripts/verify-commit.ts $1"
  },
  "lint-staged": {
    "package/src/*.{ts,tsx,js}": "eslint --cache --fix"
  }
}
